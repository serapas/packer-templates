{
  "builders": [
    {
      "accelerator": "{{ user `qemu_accelerator` }}",
      "boot_command": [
        "<esc><esc><esc>",
        "<enter><wait>",
        "/casper/vmlinuz ",
        "root=/dev/sr0 ",
        "initrd=/casper/initrd ",
        "autoinstall ds=nocloud-net;s=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ubuntu/",
        "<enter>"
      ],
      "boot_wait": "2s",
      "cpus": "{{ user `vcpus` }}",
      "disk_interface": "virtio",
      "disk_size": "{{ user `disk_size` }}",
      "format": "qcow2",
      "headless": true,
      "http_directory": "http",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_url": "{{ user `iso_url` }}",
      "memory": "{{ user `memory` }}",
      "output_directory": "output-{{ user `vm_name` }}-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "echo '/sbin/halt -h -p' > shutdown.sh; echo 'packer'|sudo -S bash 'shutdown.sh'",
      "ssh_handshake_attempts": "20",
      "ssh_password": "{{ user `ssh_password` }}",
      "ssh_timeout": "60m",
      "ssh_username": "{{ user `ssh_username` }}",
      "type": "qemu",
      "vm_name": "{{ user `vm_name` }}-{{ timestamp }}"
    },
    {
      "boot_command": [
        "<esc><esc><esc>",
        "c<wait>",
        "linux ",
        "/casper/vmlinuz ",
        "autoinstall 'ds=nocloud-net;s=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ubuntu/'",
        "<enter>",
        "initrd /casper/initrd ",
        "<enter>",
        "boot",
        "<enter>"
      ],
      "boot_wait": "5s",
      "cpus": "{{ user `vcpus` }}",
      "disk_size": "{{ user `disk_size` }}",
      "guest_os_type": "Ubuntu_64",
      "hard_drive_interface": "{{ user `disk_adapter_type` }}",
      "headless": false,
      "http_directory": "http",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_url": "{{ user `iso_url` }}",
      "memory": "{{ user `memory` }}",
      "output_directory": "output-{{ user `vm_name` }}-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "echo '/sbin/halt -h -p' > shutdown.sh; echo 'packer'|sudo -S bash 'shutdown.sh'",
      "ssh_handshake_attempts": "20",
      "ssh_password": "{{ user `ssh_password` }}",
      "ssh_timeout": "60m",
      "ssh_username": "{{ user `ssh_username` }}",
      "type": "virtualbox-iso",
      "vm_name": "{{ user `vm_name` }}-{{ timestamp }}"
    },
    {
      "boot_command": [
        "<esc><esc><esc>",
        "<enter><wait>",
        "/casper/vmlinuz ",
        "root=/dev/sr0 ",
        "initrd=/casper/initrd ",
        "autoinstall ds=nocloud-net;s=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ubuntu/",
        "<enter>"
      ],
      "boot_wait": "5s",
      "cpus": "{{ user `vcpus` }}",
      "disk_adapter_type": "{{ user `disk_adapter_type` }}",
      "disk_size": "{{ user `disk_size` }}",
      "disk_type_id": 0,
      "guest_os_type": "ubuntu-64",
      "headless": true,
      "http_directory": "http",
      "iso_checksum": "{{ user `iso_checksum` }}",
      "iso_url": "{{ user `iso_url` }}",
      "memory": "{{ user `memory` }}",
      "output_directory": "output-{{ user `vm_name` }}-{{ build_type }}-{{ timestamp }}",
      "shutdown_command": "echo '/sbin/halt -h -p' > shutdown.sh; echo 'packer'|sudo -S bash 'shutdown.sh'",
      "ssh_handshake_attempts": "20",
      "ssh_password": "{{ user `ssh_password` }}",
      "ssh_timeout": "60m",
      "ssh_username": "{{ user `ssh_username` }}",
      "type": "vmware-iso",
      "vm_name": "{{ user `vm_name` }}-{{ timestamp }}",
      "vmx_data": {
        "ethernet0.pciSlotNumber": "32"
      },
      "vmx_remove_ethernet_interfaces": true
    }
  ],
  "post-processors": [
    [
      {
        "compression_level": "{{ user `compression_level` }}",
        "output": "{{ user `vm_name` }}-{{.Provider}}-{{ timestamp }}.box",
        "type": "vagrant"
      },
      {
        "output": "manifest.json",
        "strip_path": true,
        "type": "manifest"
      }
    ]
  ],
  "provisioners": [
    {
      "scripts": [
        "scripts/base.sh",
        "scripts/desktop.sh",
        "scripts/vagrant.sh",
        "scripts/virtualbox.sh",
        "scripts/vmware.sh",
        "scripts/sshd.sh",
        "scripts/cleanup.sh",
        "scripts/zerodisk.sh"
      ],
      "type": "shell"
    }
  ],
  "variables": {
    "boot_command_prefix": "",
    "box_tag": "",
    "compression_level": "6",
    "disk_adapter_type": "scsi",
    "disk_size": "36864",
    "iso_checksum": "",
    "iso_url": "",
    "memory": "1024",
    "qemu_accelerator": "hvf",
    "ssh_password": "vagrant",
    "ssh_username": "vagrant",
    "vagrant_cloud_token": "",
    "vagrant_cloud_username": "",
    "vcpus": "1",
    "vm_name": ""
  }
}
